{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Seekbar Timer",
  "version": "1.0.0",
  "identifier": "com.yokenzan.seekbar-timer",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Seekbar Timer",
        "width": 50,
        "height": 1080,
        "x": 1870,
        "y": 0,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "decorations": false,
        "transparent": true,
        "skipTaskbar": true,
        "shadow": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
